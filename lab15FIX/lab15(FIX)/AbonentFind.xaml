<Window x:Class="lab15_FIX_.AbonentFind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab15_FIX_"
        mc:Ignorable="d"
        Title="AbonentFind" Height="220" Width="500">
    <Grid>
        <TextBox x:Name="NameBox" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="445" Height="24"/>
        <Label Content="Введите имя абонента, которого хотите найти" HorizontalAlignment="Left" Margin="55,55,0,0" VerticalAlignment="Top" Height="30" Width="355"/>
        <Button x:Name="FindButton" Content="Поиск" HorizontalAlignment="Left" Margin="55,129,0,0" VerticalAlignment="Top" Height="40" Width="110" Click="FindButton_Click"/>
        <Button Content="Отменить" HorizontalAlignment="Left" Margin="330,129,0,0" VerticalAlignment="Top" Height="40" Width="110" IsCancel="True"/>
    </Grid>
</Window>
